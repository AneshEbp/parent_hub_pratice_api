{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@api": [
        "apps/api/src"
      ],
      "@api/*": [
        "apps/api/src/*"
      ],
      "@cms-api": [
        "apps/cms-api/src"
      ],
      "@cms-api/*": [
        "apps/cms-api/src/*"
      ],
      "@app/libs": [
        "libs/libs/src"
      ],
      "@app/libs/*": [
        "libs/libs/src/*"
      ],
      "@app/data-access": [
        "libs/data-access/src"
      ],
      "@app/data-access/*": [
        "libs/data-access/src/*"
      ],
      "@app/authentication": [
        "libs/authentication/src"
      ],
      "@app/authentication/*": [
        "libs/authentication/src/*"
      ],
      "@app/email": [
        "libs/email/src"
      ],
      "@app/email/*": [
        "libs/email/src/*"
      ],
      "@app/common": [
        "libs/common"
      ],
      "@app/common/*": [
        "libs/common/*"
      ],
      "@app/social-auth": [
        "libs/social-auth/src"
      ],
      "@app/social-auth/*": [
        "libs/social-auth/src/*"
      ],
      "@app/fcm": [
        "libs/fcm/src"
      ],
      "@app/fcm/*": [
        "libs/fcm/src/*"
      ]
    }
  }
}